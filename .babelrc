{
  "presets": [
    "react",
    "stage-0",
    "es2015"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "helpers": false, 
        "polyfill": false,  
        "regenerator": true, 
        "moduleName": "babel-runtime" 
      }
    ]
  ]
}